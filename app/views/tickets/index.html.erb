
<div class="row">
  <div class="col-md-10 offset-md-2">
    <h3>Users Listing</h3>
  </div>
</div>  

<div class="row">

    <% @tickets.each do |s| %>
      <div class="col-md-8 offset-md-2">
        <div class="card">
          <div class="card-body">
            <span class="card-title">User ID: <%= s.user_id %></span>
            <span class="card-title">Creation: <%= s.created_at %></span>
            <span class="card-title">Description: <%= s.description %></span>
            <span class="card-title">Response: <%= s.response %></span>
            <span class="card-title">Status: <%= s.status %></span>
          </div>
        </div>
      </div>
    <% end %>  
  
</div>




